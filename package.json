    "type": "git",
    "url": "git+https://github.com/anitawlosek/habitica-avatar-react.git"
  },
  "bugs": {
    "url": "https://github.com/anitawlosek/habitica-avatar-react/issues"
  },
  "homepage": "https://github.com/anitawlosek/habitica-avatar-react#readme",
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@eslint/js": "^9.37.0",
    "@storybook/addon-a11y": "^9.1.10",
    "@storybook/addon-docs": "^9.1.10",
    "@storybook/addon-vitest": "^9.1.10",
    "@storybook/react": "^9.1.10",
    "@storybook/react-vite": "^9.1.10",
    "@testing-library/jest-dom": "^6.9.0",
    "@testing-library/react": "^16.3.0",
    "@types/moment": "^2.13.0",
    "@types/node": "^24.6.0",
    "@types/react": "^19.2.0",
    "@types/storybook__react": "^5.2.1",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/browser": "3.2.4",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.37.0",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.4.0",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.55.1",
    "sass": "^1.93.2",
    "storybook": "^9.1.10",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7",
