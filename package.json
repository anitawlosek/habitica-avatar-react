{
  "name": "habitica-avatar-react",
  "version": "0.0.2-alpha",
  "description": "A React component library for rendering Habitica game avatars with customizable appearance, gear, pets, mounts, and visual effects",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "npm test",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "sass:build": "sass src:src --no-source-map",
    "sass:watch": "sass --watch src:src --no-source-map",
    "dev": "npm-run-all --parallel sass:watch storybook",
    "prebuild": "rm -rf dist && npm run sass:build",
    "build": "tsup src/index.ts --dts --format esm,cjs --out-dir dist"
  },
  "keywords": [
    "react",
    "habitica",
    "avatar",
    "component",
    "typescript",
    "game"
  ],
